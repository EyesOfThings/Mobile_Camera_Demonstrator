<link href="//vjs.zencdn.net/5.19/video-js.min.css" rel="stylesheet">
<div id="image_processing">
  <img class="loading-one" src="/assets/loading.gif"/>
</div>
<%= render 'shared/menu' %>

<div class="row">
  <div class="ui message transition no-animations" style="display: none;">
    <i class="close icon"></i>
    <p>You have nothing to show.</p>
  </div>
</div>

<div class="row-10">
  <div class="ui cards animate-gallery">
    
  </div>
</div>

<script src="//vjs.zencdn.net/5.19/video.min.js"></script>
<script>
  window.CurrentUser = <%= @current_user.to_json.html_safe %>;
  window.AuthData = <%= @auth_data.to_json.html_safe %>;
  $(document).ready(function () {
      $('.message .close')
    .on('click', function() {
      $(this)
        .closest('.message')
        .transition('fade')
      ;
    })
  ;
    $('.ui.dropdown').dropdown();
    $('.ui.radio.checkbox')
      .checkbox()
    ;
    window.initializeAnimations();
  });
</script